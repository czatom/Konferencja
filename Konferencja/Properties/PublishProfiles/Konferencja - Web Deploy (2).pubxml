    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="True">
          <Destination Path="Data Source=tcp:konferencja.database.windows.net,1433;Initial Catalog=Konferencja_db;User ID=dbadmin@konferencja;Password=Konfagh*" Name="Data Source=tcp:konferencja.database.windows.net,1433;Initial Catalog=Konferencja_db;User Id=dbadmin@konferencja;Password=Konfagh*" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="Konferencja.Models.ApplicationDbContext, Konferencja" MigrationConfiguration="Konferencja.Migrations.Configuration, Konferencja" Origin="Configuration" />
          </Object>
        </ObjectGroup>
