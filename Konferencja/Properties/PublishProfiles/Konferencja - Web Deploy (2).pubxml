  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:konferencja.database.windows.net,1433;Initial Catalog=Konferencja_db;User Id=dbadmin@konferencja;Password=Konfagh*</ParameterValue>
