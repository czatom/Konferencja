@model IEnumerable<Konferencja.Models.Publication>

        <br />
        @foreach (var pub in Model)
        {
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.ApplicationUser.FullName)
                </dt>

                <dd>
                    @Html.DisplayFor(model => pub.ApplicationUser.FullName)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Title)
                </dt>

                <dd>
                    @Html.DisplayFor(model => pub.Title)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Description)
                </dt>

                <dd>
                    @Html.DisplayFor(model => pub.Description)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.File)
                </dt>

                <dd>
                    @Html.ActionLink("Pobierz", null, null, null, new { href = pub.File })
                </dd>

            </dl>
            <hr />
        }

